{
    "info": {
        "name": "Public Module",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Create Public Content",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{token}}",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"content\": \"<h1>Privacy Policy</h1>\",\n  \"type\": \"privacy-policy\"\n}",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{baseUrl}}/public",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "public"
                    ]
                }
            },
            "response": []
        },
        {
            "name": "Get All Publics By Type",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/public/:type",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "public",
                        ":type"
                    ],
                    "variable": [
                        {
                            "key": "type",
                            "value": "privacy-policy"
                        }
                    ]
                }
            },
            "response": []
        },
