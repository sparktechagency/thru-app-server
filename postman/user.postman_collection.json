    "item": [
        {
            "name": "Get Profile",
            "request": {
                "method": "GET",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{token}}",
                        "type": "text"
                    }
                ],
                "url": {
                    "raw": "{{baseUrl}}/user/profile",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "user",
                        "profile"
                    ]
                }
            },
            "response": []
        },
        {
            "name": "Update Profile",
            "request": {
                "method": "PATCH",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{token}}",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"name\": \"John\",\n  \"lastName\": \"Doe\",\n  \"address\": \"123 Main St\",\n  \"bio\": \"Hello world\",\n  \"longitude\": -73.935242,\n  \"latitude\": 40.730610\n}",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{baseUrl}}/user/profile",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "user",
                        "profile"
                    ]
                }
            },
            "response": []
        },
